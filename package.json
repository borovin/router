{
  "name": "@basket/router",
  "description": "Simple SPA router with dynamic page loading.",
  "version": "2.0.1",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:codestyle": "eslint ."
  },
  "devDependencies": {
    "babel-jest": "^16.0.0",
    "babel-preset-es2015": "^6.16.0",
    "eslint": "^3.8.1",
    "jest": "^16.0.2",
    "jsdom": "latest"
  },
  "jest": {
    "testURL": "http://localhost/test/path?a=1&b=b&c=false#hash",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!coverage/**"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "author": "a@borovin.com",
  "license": "MIT",
  "dependencies": {
    "lodash.defaults": "^4.2.0",
    "lodash.foreach": "^4.5.0",
    "qs": "^6.3.0",
    "route-parser": "0.0.5"
  }
}
